{
    "name": "micro/plugin-http-pack",
    "description": "Micro Framework: HTTP plugins pack.",
    "license": "MIT",
    "type": "micro-plugin",
    "authors": [
        {
            "name": "Stanislau Komar",
            "email": "kost@micro-php.net"
        }
    ],
    "require": {
        "micro/kernel-boot-plugin-depended": "^2.0",
        "micro/plugin-http-boot": "^2.0",
        "micro/plugin-http-exceptions": "^2.0",
        "micro/plugin-http-exceptions-dev": "^2.0",
        "micro/plugin-http-logger": "^2.0",
        "micro/plugin-http-middleware": "^2.0",
        "micro/plugin-http-router-code": "^2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "Micro\\Plugin\\Http\\": "/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
